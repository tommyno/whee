{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["airbnb", "airbnb/hooks", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/jsx-props-no-spreading": 0,
    "prettier/prettier": "error"
  },
  "globals": {
    "React": "writable"
  }
}
